/*Global*/

.container {padding: 0;}

#wrapper {
  min-height: 100%;
  height: auto !important;
  height: 100%;
  margin: 0 auto -{{ settings.footer_height }};
}

#push, #wrapper-footer {height: {{ settings.footer_height }};}
html, body {height: 100%;}

body{
  {% case settings.body_bg_type %}
    {% when 'bg_image' %}
          background: url("{{ 'body_bg.png' | asset_url }}") repeat left top transparent !important;
    {% when 'bg_color' %}
      background-color: {{ settings.body_bg_color }}
  {% endcase %}
}

/*Typography*/

